{
    "presets": [
      [
        "@babel/preset-env", 
        {
          "useBuiltIns": "entry"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      ["@babel/plugin-proposal-class-properties", { "loose": true }],
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ],
      "react-hot-loader/babel"
    ]
  
}